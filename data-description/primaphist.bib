@article{andrewGlobalCO2Emissions2018,
  title = {Global {{CO}}{\textsubscript{2}} Emissions from Cement Production},
  volume = {10},
  issn = {1866-3508},
  doi = {https://doi.org/10.5194/essd-10-195-2018},
  abstract = {$<$p$><$strong$>$Abstract.$<$/strong$>$ The global production of cement has grown very rapidly in recent years, and after fossil fuels and land-use change, it is the third-largest source of anthropogenic emissions of carbon dioxide. The required data for estimating emissions from global cement production are poor, and it has been recognised that some global estimates are significantly inflated. Here we assemble a large variety of available datasets and prioritise official data and emission factors, including estimates submitted to the UNFCCC plus new estimates for China and India, to present a new analysis of global process emissions from cement production. We show that global process emissions in 2016 were 1.45$\pm$0.20Gt CO\textsubscript{2}, equivalent to about 4\% of emissions from fossil fuels. Cumulative emissions from 1928 to 2016 were 39.3$\pm$2.4Gt CO\textsubscript{2}, 66\% of which have occurred since 1990. Emissions in 2015 were 30\% lower than those recently reported by the Global Carbon Project. The data associated with this article can be found at $<$a href="https://doi.org/10.5281/zenodo.831455" target="\_blank"$>$https://doi.org/10.5281/zenodo.831455$<$/a$>$.$<$/p$>$},
  language = {English},
  number = {1},
  journal = {Earth System Science Data},
  author = {Andrew, Robbie M.},
  month = jan,
  year = {2018},
  pages = {195-217},
  file = {/home/johannes/Zotero/storage/IQY4MI7Z/Andrew - 2018 - Global COsub2sub emissions from cement produc.pdf;/home/johannes/Zotero/storage/VUZ3XXFB/2018.html}
}

