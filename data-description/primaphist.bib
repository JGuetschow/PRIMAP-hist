@article{KleinGoldewijk2016a,
  title = {New Anthropogenic Land Use Estimates for the {{Holocene}}; {{HYDE}} 3.2},
  issn = {1866-3591},
  doi = {10.5194/essd-2016-58},
  abstract = {This paper presents an update and expansion of the History Database of the Global Environment (HYDE, v 3.2.000). HYDE is and internally consistent combination of updated historical population estimates and enhanced allocation algorithms with weighting maps for land use which are time-dependent. Categories include cropland, with a new distinction into irrigated and rain fed crops (other than rice) and irrigated and rain fed rice. Also grazing lands are provided, divided into more intensively used pasture and less intensively used rangeland. Population is represented by maps of total, urban, rural population and population density as well as built-up area. The period covered is 10\&thinsp;000\&thinsp;BCE to 2015\&thinsp;CE. We estimate that global population increased from 4.4 million people in 10\&thinsp;000 BCE to 7310\&thinsp;million in 2015\&thinsp;CE, resulting in a global population density increase of \&minus;2 to almost 56\&thinsp;cap\&thinsp;km\&minus;2 respectively. The urban built-up area evolved from almost zero to roughly 450\&thinsp;Mha at present, still only less than 0.5\&thinsp;\% of the total land surface of the globe. Cropland occupied approximately less than 1\&thinsp;\% of the global land area for a long time period until 1000\&thinsp;CE, quite similar like the grazing land area. In the next centuries the share of global cropland slowly grew to 2.3\&thinsp;\% in 1700\&thinsp;CE (ca. 294\&thinsp;Mha), 4.5\&thinsp;\% in 1850\&thinsp;CE (580\&thinsp;Mha) and 11\&thinsp;\% in 2015\&thinsp;CE (ca. 1564\&thinsp;Mha). Cropland can be further divided into rain-fed and irrigated land, and these categories can be further divided into rice and non-rice. Rainfed croplands are much more common with 2.2\&thinsp;\% in 1700\&thinsp;CE (294\&thinsp;Mha), 4.2\&thinsp;\% (552\&thinsp;Mha) in 1850\&thinsp;CE and 10\&thinsp;\% (1298\&thinsp;Mha) in 2015 CE, while irrigated croplands used less than 0.05\&thinsp;\% (5\&thinsp;Mha), 0.2\&thinsp;\% (28\&thinsp;Mha) and 2.0\&thinsp;\% in 2015\&thinsp;CE (266\&thinsp;Mha) resp. We estimate the irrigated rice area (paddy) at 0.02\&thinsp;\% in 1700\&thinsp;CE (2.7\&thinsp;Mha), 0.1\&thinsp;\% (127\&thinsp;Mha) in 1850\&thinsp;CE and 0.6\&thinsp;\% in 2015\&thinsp;CE (73\&thinsp;Mha). The share of grazing land grew from 5.1\&thinsp;\% in 1700\&thinsp;CE (668\&thinsp;Mha) to 9.2\&thinsp;\% in 1850\&thinsp;CE (1200\&thinsp;Mha) and 25.2\&thinsp;\% in 2015\&thinsp;CE (3284\&thinsp;Mha). Land used for grazing can be divided into more intensively used pasture and less (or not) managed rangeland. Pasture occupied 1.1\&thinsp;\% in 1700\&thinsp;CE (149\&thinsp;Mha), 2.0\&thinsp;\% in 1850\&thinsp;CE (257\&thinsp;Mha) and 6.1\&thinsp;\% (800\&thinsp;Mha) in 2015\&thinsp;CE, while rangelands usually occupy more space due to the occurrence in more arid regions and thus lower yields to sustain livestock. We estimate 4\&thinsp;\% in 1700\&thinsp;CE (519\&thinsp;Mha), 7.2\&thinsp;\% in 1850\&thinsp;CE (943\&thinsp;Mha) and 19\&thinsp;\% in 2015\&thinsp;CE (2483\&thinsp;Mha). All data can be downloaded from doi:10.17026/dans-znk-cfy3.},
  journal = {Earth System Science Data Discussions},
  author = {Klein Goldewijk, Kees and Beusen, Arthur and Doelman, Jonathan and Stehfest, Elke},
  year = {2016},
  pages = {1-40},
  file = {/home/johannes/Documents/paper_db/2016/Klein Goldewijk et al. - 2016 - New anthropogenic land use estimates for the Holocene HYDE 3.2.zip;/home/johannes/Documents/paper_db/2016/Klein Goldewijk et al. - 2016 - New anthropogenic land use estimates for the Holocene HYDE 3.2(2).pdf}
}

